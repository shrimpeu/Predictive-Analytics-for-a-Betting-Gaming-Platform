   "source": [
    "#efficiency metrics\n",
    "\n",
    "merged_df['Offensive_Efficiency_HOME'] = merged_df['PTS_HOME'] / merged_df['REB_TOTAL_HOME']\n",
    "merged_df['Defensive_Efficiency_HOME'] = merged_df['PTS_AWAY'] / merged_df['REB_TOTAL_HOME']\n",
    "merged_df['Offensive_Efficiency_AWAY'] = merged_df['PTS_AWAY'] / merged_df['REB_TOTAL_AWAY']\n",
    "merged_df['Defensive_Efficiency_AWAY'] = merged_df['PTS_HOME'] / merged_df['REB_TOTAL_AWAY']\n",
    "\n",
    "# pace\n",
    "merged_df['Pace_Home'] = merged_df['FGA_HOME'] + merged_df['FGA_AWAY']\n",
    "merged_df['Pace_Away'] = merged_df['FGA_AWAY'] + merged_df['FGA_HOME']\n",
    "\n",
    "# Calculate possessions for Home and Away teams\n",
    "merged_df['Home_Possessions'] = merged_df['FGA_HOME'] + 0.44 * merged_df['FTA_HOME'] - merged_df['OREB_HOME'] + merged_df['TOV_HOME']\n",
    "merged_df['Away_Possessions'] = merged_df['FGA_AWAY'] + 0.44 * merged_df['FTA_AWAY'] - merged_df['OREB_AWAY'] + merged_df['TOV_AWAY']\n",
    "\n",
    "# Offensive Rating for Home and Away teams\n",
    "merged_df['ORtg_Home'] = (merged_df['Home_PTS_RollAvg'] / merged_df['Home_Possessions']) * 100\n",
    "merged_df['ORtg_Away'] = (merged_df['Away_PTS_RollAvg'] / merged_df['Away_Possessions']) * 100\n",
    "\n",
    "# Defensive Rating for Home and Away teams\n",
    "merged_df['DRtg_Home'] = (merged_df['Away_PTS_RollAvg'] / merged_df['Away_Possessions']) * 100\n",
    "merged_df['DRtg_Away'] = (merged_df['Home_PTS_RollAvg'] / merged_df['Home_Possessions']) * 100\n",
    "\n",
    "# Calculating Net Rating for Home and Away Teams\n",
    "merged_df['NetRtg_Home'] = merged_df['ORtg_Home'] - merged_df['DRtg_Home']\n",
    "merged_df['NetRtg_Away'] = merged_df['ORtg_Away'] - merged_df['DRtg_Away']\n",
    "\n",
    "# Display the DataFrame with the calculated ratings\n",
    "print(merged_df[['Home_PTS_RollAvg', 'Away_PTS_RollAvg', 'Home_Possessions', 'Away_Possessions', 'ORtg_Home', 'ORtg_Away', 'DRtg_Home', 'DRtg_Away', 'NetRtg_Home', 'NetRtg_Away']])\n"
   ]
